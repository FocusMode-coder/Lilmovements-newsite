{
  "name": "lily-movements-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:resolve": "prisma migrate resolve --applied 20251123000000_init_postgresql",
    "migrate:safe-deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "deploy": "npm run migrate:safe-deploy && npm run build",
    "build:production": "prisma generate && npm run migrate:safe-deploy && next build",
    "audit:fix": "npm audit fix --force"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.3",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.0.0",
    "framer-motion": "^10.18.0",
    "next": "14.2.33",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
